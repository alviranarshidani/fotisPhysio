<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="fotisPhysio.Home">
    <ContentPage.Content>
        <Grid>
        <StackLayout Grid.Row="0" VerticalOptions="Center">
                <Entry Placeholder="Enter First Name" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Enter Last Name" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Enter Email" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Enter Password" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Confirm Password" Margin="10,10,10,10"></Entry>
            </StackLayout>
            <StackLayout Grid.Row="1" VerticalOptions="StartAndExpand">
                <Button Text="Register" HorizontalOptions="Center"></Button>
            </StackLayout>
            </Grid>
    </ContentPage.Content>
</ContentPage>
