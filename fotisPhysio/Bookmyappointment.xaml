<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="fotisPhysio.Bookmyappointment" Title="Book Appointment">
    <ContentPage.Content>
        <Grid>
        <StackLayout Grid.Row="0" VerticalOptions="Center">
                <Entry Placeholder="Enter Name" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Enter Email ID" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Enter Phone Number" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Current Patient" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Type of Appointment" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Preferred Appointment Date" Margin="10,10,10,10"></Entry>
                <Entry Placeholder="Preferred Appointment Time" Margin="10,10,10,10"></Entry>
                
                
                
            <Button Text="Submit" Clicked="Submit_Clicked"></Button>
            </StackLayout>
                    </Grid>
    </ContentPage.Content>
</ContentPage>
